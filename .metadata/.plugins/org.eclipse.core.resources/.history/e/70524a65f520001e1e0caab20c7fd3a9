package atm;

public class Client {
	//attributes
	private String name;
	private int code;
	
	//constructeur
	public Client() {};
	
	
	public Client(String pName, int pCode) {
		this.name = pName;
		this.code = pCode;
	}
	
	//methode
	public void setName(String pName) {
		this.name = pName;
	}
	
	public String getName() {
		return name;
	}
	
	public void setCode(int pCode) {
		this.code = pCode;
	}
	
	public int getCode() {
		return code;
	}
	
}
